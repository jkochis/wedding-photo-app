{
  "name": "wedding-photo-app",
  "version": "1.0.0",
  "type": "module",
  "description": "A mobile-first wedding photo sharing app with TypeScript, face detection, and secure access control",
  "main": "server/index.cjs",
  "scripts": {
    "start": "node server/index.cjs",
    "start:ts": "npx ts-node --project tsconfig.node.json server/index.ts",
    "dev": "npm run build && npm start",
    "build": "npm run build:frontend",
    "build:frontend": "tsc && cp dist/public/js/*.js public/js/ && cp dist/public/js/*.js.map public/js/ 2>/dev/null || true",
    "build:static": "node scripts/build-static.js",
    "build:production": "npm run build && npm run build:static",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf uploads/* && echo '[]' > server/photos.json",
    "css:list": "node scripts/css-utils.js list",
    "css:analyze": "node scripts/css-utils.js analyze",
    "css:component": "node scripts/css-utils.js component",
    "css:utility": "node scripts/css-utils.js utility",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["wedding", "photos", "typescript", "mobile", "pwa", "face-detection"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.12.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
